include <stdbool.h>
#include <string.h>
bool isValid(char* s) 
{
    int newlength;
    int length = strlen(s), number;
    do 
    {
        number = 0;
        length = strlen(s);
        for (int i = 0;i<length; i++) 
        {
          if ((s[i] == '('&&s[i + 1] ==')') || (s[i] == '[' &&s[i + 1] ==']') || (s[i] == '{' &&s[i + 1] =='}')) 
          {
                memmove(s + i, s + i + 2, length - i - 1);
                number= 1;
                s[length - 2] = '\0';
                break; 
        }
        }
    } 
    while (number!=0);
    newlength = strlen(s);
    if(newlength==0)
    return true;
    else 
    return false;
}
